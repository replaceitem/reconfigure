{
    "schemaVersion": 1,
    "id": "reconfigure",
    "version": "${version}",
    "name": "Reconfigure",
    "description": "replaceitem's config library",
    "authors": [
        "replaceitem"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/reconfigure",
        "issues": "https://github.com/replaceitem/reconfigure/issues",
        "sources": "https://github.com/replaceitem/reconfigure"
    },
    "license": "MIT",
    "icon": "assets/reconfigure/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "net.replaceitem.reconfigure.Reconfigure"
        ]
    },
    "mixins": [
        "reconfigure.mixins.json"
    ],
    "accessWidener": "reconfigure.accesswidener",
    "depends": {
        "fabricloader": ">=0.16.10",
        "fabric-api": ">=0.127.0",
        "minecraft": ">=${minecraft_version}"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "https://discord.gg/etTDQAVSgt"
            }
        },
        "badges": [ "library" ]
    }
